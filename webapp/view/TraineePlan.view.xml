<mvc:View
	controllerName="com.dardan.rrafshi.traineehub.controller.TraineePlan"
	displayBlock="true"
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc">
	<Page enableScrolling="false">
		<customHeader>
			<OverflowToolbar>
				<Button icon="sap-icon://nav-back" press="onBackward"/>
				<ToolbarSpacer/>
				<Title text="{i18n>appTitle}"/>
				<ToolbarSpacer/>
				<Avatar
					src="{appData>/userInfo/picture}"
					displaySize="XS"
					displayShape="Circle"
					press="onProfileClicked"/>
			</OverflowToolbar>
		</customHeader>
		<content>
			<Text text="Trainee Plan"/>
		</content>
	</Page>
</mvc:View>
